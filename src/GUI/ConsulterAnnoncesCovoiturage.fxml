<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="660.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ConsulterAnnoncesCovoiturageController">
    <stylesheets>
        <URL value="@/Asset/consultertoutesannonces.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableannonces" layoutY="110.0" onMouseClicked="#setCellValueFromTableToText" prefHeight="200.0" prefWidth="714.0">
         <columns>
            <TableColumn fx:id="Datedepart" prefWidth="127.0" text="Date de Depart" />
            <TableColumn fx:id="Datearrivee" prefWidth="134.0" text="Date d' Arrivée" />
            <TableColumn fx:id="Adressedepart" minWidth="9.0" prefWidth="156.0" text="Adresse de Depart" />
            <TableColumn fx:id="Adressearrivee" minWidth="0.0" prefWidth="154.0" text="Adresse d'Arrivée" />
            <TableColumn fx:id="Tarrifs" minWidth="0.0" prefWidth="141.0" text="Tarif" />
            <TableColumn fx:id="idannonce" prefWidth="75.0" text="Column X" visible="false" />
            <TableColumn fx:id="idannonceur" prefWidth="75.0" text="Column X" visible="false" />
         </columns>
      </TableView>
      <Label layoutX="255.0" layoutY="25.0" text="Consulter les annonces de covoiturage" textAlignment="CENTER" />
      <Button fx:id="btn_conlt_itin" layoutX="102.0" layoutY="335.0" mnemonicParsing="false" onAction="#ConsulterItinerarire" prefHeight="25.0" prefWidth="170.0" text="Consulter Itineraire " />
      <Button fx:id="btn_envoyer_mail" layoutX="370.0" layoutY="335.0" mnemonicParsing="false" onAction="#EnvoyerMail" prefHeight="25.0" prefWidth="128.0" text="Envoyer Email" />
      <Button fx:id="btn_retour" layoutX="648.0" layoutY="335.0" mnemonicParsing="false" onAction="#Retour" text="Retour" />
   </children>
</AnchorPane>
